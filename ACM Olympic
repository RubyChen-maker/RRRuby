#include<stdio.h>
int main()
{	
 int month[9]={0,31,29,31,30,31,30,31,8};	
 int t,m,d,h,i,j;	
 scanf("%d",&t);	
 while(t--)
 {		
  h=0;		
  scanf("%d %d",&m,&d);		
  for(i=m;i<=8;i++)
  {			
   if(i==m)			
   h+=month[i]-d;			
   else			
   h+=month[i];		
  }		
  printf("%d\n",h);	
 }	
return 0;
} 

